    "# method to print the divisors \n",
    "def listeDivisors(n) :\n",
    "    l = []\n",
    "    i = 1\n",
    "    while i <= n : \n",
    "        if (n % i==0) : \n",
    "            l.append(i)\n",
    "        i = i + 1\n",
    "    return l"
   ]
